{
    "*.php": [
        "npm run check:php:compatibility:56+",
        "npm run check:php:compatibility:wp:56+",
        "npm run lint:wpcs"
    ],
    "*.css": [
        "npm run lint:css:fix",
        "npm run lint:css"
    ],
    "*.json": [
        "npm run lint:pkg:json"
    ],
    "*.md": [
        "npm run lint:readme:md"
    ]
}